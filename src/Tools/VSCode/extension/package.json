                    }
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/mocha": "^9.1.0",
        "@types/node": "^17.0.19",
        "@types/vscode": "^1.65.0",
        "mocha": "^9.2.1",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "vscode-languageclient": "~7.0.0",
        "vscode-languageserver-types": "~3.16.0"
    }
}
